<jdbc-perf-logger>
  <local-server port="8089" />
  <jdbc-drivers>
    <jdbc-driver>
      <prefix>jdbc:hsqldb:</prefix>
      <driver-class-name>org.hsqldb.jdbc.JDBCDriver</driver-class-name>
    </jdbc-driver>
    <jdbc-driver>
      <prefix>jdbc:mysql:</prefix>
      <driver-class-name>com.mysql.jdbc.Driver</driver-class-name>
    </jdbc-driver>
    <jdbc-driver>
      <prefix>jdbc:sqlserver:</prefix>
      <driver-class-name>com.microsoft.sqlserver.jdbc.SQLServerDriver</driver-class-name>
    </jdbc-driver>
  </jdbc-drivers>
</jdbc-perf-logger>
