<?xml version="1.0" encoding="utf-8"?>
<rules>
	<rule>
		<from>^/de/index$</from>
		<to last="true">/en/index.jsp</to>
	</rule>
	<rule>
		<from>^/en/index.jsp$</from>
		<to type="redirect" last="true">/de/index</to>
	</rule>
	<rule>
		<from>^/de/error$</from>
		<to last="true">/de/fehler.jsp</to>
	</rule>
</rules>