<?xml version="1.0" encoding="utf-8"?>
<rules>
	<rule>
		<from>^/fr/accueil$</from>
		<to last="true">/fr/index.jsp</to>
	</rule>
	<rule>
		<from>^/fr/index.jsp$</from>
		<to type="redirect" last="true">/fr/accueil</to>
	</rule>
	<rule>
		<from>^/de/fault((\?\S+)?)$</from>
		<to last="true">/de/fehler.jsp$1</to>
	</rule>
</rules>